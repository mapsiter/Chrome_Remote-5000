<?xml version="1.0" encoding="utf-8"?>
<layout onLaunch="launch" onVolumeUp="volume_up" onVolumeDown="volume_down" color="#1D3461">
	<grid>
		<row>
			<button icon="power" onTap="shutdown" color="#EF233C" />
			<button icon="fullscreen" onTap="fullscreen" />
		</row>
		<row>
			<button icon="zoomout" onTap="zoom_out" />
			<button text="100%" onTap="zoom_normal" />
			<button icon="zoomin" onTap="zoom_in" />
		</row>
		<row>
			<button icon="search" onTap="find" />
			<button icon="home" onTap="home" />
			<button icon="web" onTap="address" />
		</row>
		<row>
			<button icon="docnew" onTap="new_tab" />
			<button icon="docleft" onTap="previous_tab" color="#1F487E" />
			<button icon="docright" onTap="next_tab" color="#1F487E" />
		</row>
		<row>
			<button icon="docclose" onTap="close_tab" color="#EF233C" />
			<button icon="left" onTap="back" color="#1F487E" />
			<button icon="right" onTap="forward" color="#1F487E" />
		</row>
		<row>
			<button icon="rwd" onTap="rewind" />
			<button icon="playpause" onTap="play_pause" color="#519872" />
			<button icon="ff" onTap="forward" />
		</row>
		<row>
			<button icon="refresh" onTap="refresh" color="#519872" />
			<button icon="up" onTap="scroll_up" />
			<button icon="down" onTap="scroll_down" />
		</row>
	</grid>
</layout>
